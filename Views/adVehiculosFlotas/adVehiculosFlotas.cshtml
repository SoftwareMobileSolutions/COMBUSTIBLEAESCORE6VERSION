            alert: function (title, text, icon) {
                Swal.fire({
                    icon: icon,
                    title: title,
                    text: text,
                });
            }, 
            init: function () {
                var _this = this;
                _this.tabstrip()
                _this.vehiculo.DropDowns.loadDropDowns();
                _this.vehiculo.GRID.loadGrid();
                _this.vehiculo.GRID.BTNGeneraGrid()
                _this.vehiculo.BTN_RegistrarVehiculo();
                _this.vehiculo.keypress();
