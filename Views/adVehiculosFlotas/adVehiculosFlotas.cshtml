                    }

                    return true
                },
                nota: function (mensaje, estado) {
                    var nota = $("#nota").val()

                    if (estado === "warning") {
                        $("#nota").text("Alerta: " + mensaje);
                        $("#nota").css("color", "red")
                        $("#Flota_Modal").css("border", "1px solid red");
                        $("#Flota_Modal").css("background-color", "#FCC6D3")
