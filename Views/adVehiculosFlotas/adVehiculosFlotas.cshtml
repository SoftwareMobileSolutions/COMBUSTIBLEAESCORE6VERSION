        height:37px;
    }

    #btn_nuevaFlota, #btn_nuevoCentroCosto {
        height: 35px;
    }

    .k-tabstrip > .k-content {
        background: #f6f9ff;
    }

    .k-grid-Editar {
        background: #198754;
        color: white;
    }

        .k-grid-Editar:hover {
            background: #157347;
        }

    .k-grid-Eliminar {
        background: #dc3545;
        color: white;
    }

        .k-grid-Eliminar:hover {
            background: #bb2d3b;
            color: white;
        }

