<script>
    (function () {
        $.extend({
            tabstrip: function () {
                $("#tabstrip").kendoTabStrip({
                    animation: {
                        open: {
                            effects: "fadeIn"
                        }
                    },
                });
            },
            vehiculo: {
                DropDowns: {
                    loadDropDowns: function () {

                        var _this = this

                        $("#Drop_Gasolina").kendoDropDownList({
                            optionLabel: 'Seleccione un tipo de gasolina',
                            dataTextField: "tipoCombustible",
                            dataValueField: "combustibleMobileID",
                            theme: "black"
                        });

                        $("#Drop_Flota").kendoDropDownList({
                            filter: "contains",
                            optionLabel: 'Seleccione la flota',
                            dataTextField: "nombreSubfleet",
                            dataValueField: "subfleetID"
                        });

                        $("#DropDown_FlotaModal").kendoDropDownList({
                            filter: "contains",
                            optionLabel: 'Seleccione la flota',
                            dataTextField: "nombreSubfleet",
                            dataValueField: "subfleetID",
                            change: function () {
                                $.vehiculo.ActualizarVehiculo.activarButton()
                            }
                        });

                        $("#DropDown_GasolinaModal").kendoDropDownList({
                            optionLabel: 'Seleccione un tipo de gasolina',
                            dataTextField: "tipoCombustible",
                            dataValueField: "combustibleMobileID",
                            change: function () {
                                //console.log("Xd")
                                $.vehiculo.ActualizarVehiculo.activarButton()
                            }
                        });
                        _this.DataDropDowns()

                    },
                    DataDropDowns: function () {
                        $.get('@Url.Action("obtenerTipoCombustible", "adVehiculosFlotas")', function (GAS) {
                            $("#Drop_Gasolina").data("kendoDropDownList").dataSource.data(GAS);
                            $("#DropDown_GasolinaModal").data("kendoDropDownList").dataSource.data(GAS);
                        })

                        $.get('@Url.Action("obtenerFlotas", "adVehiculosFlotas")', function (flotas) {
                            $("#Drop_Flota").data("kendoDropDownList").dataSource.data(flotas);
                            $("#DropDown_FlotaModal").data("kendoDropDownList").dataSource.data(flotas);
                        })
                    },
                },
                GRID: {
                    /*Fecha: {
                        FechaInicio : null,
                        FechaFin: null
                    },*/
                    loadGrid: function () {
                        var _this = this;
                        $("#FechaIni").kendoDateTimePicker({
                            dateInput: true,
                            startTime: new Date(2023, 1, 3, 8, 30, 0),
                            endTime: new Date(2023, 1, 3, 17,00, 0),
                            /*change: function (event) {
                                var data = this.value()
                                _this.Fecha.FechaInicio = moment(data).format("YYYY/MM/DD HH:mm:ss")
                                //console.log(moment(data).format("YYYY/MM/DD HH:mm:ss"))
                            }*/
                        });
                        $("#FechaIni").data("kendoDateTimePicker").value(kendo.parseDate(moment().subtract(30, 'days').format("YYYY/MM/DD HH:mm:ss")));


                        $("#FechaFin").kendoDateTimePicker({
                            dateInput: true,
                            startTime: new Date(2023, 1, 3, 8, 30, 0),
                            endTime: new Date(2023, 1, 3, 17,00, 0),
                            /*change: function (event) {
                                var data = this.value()
                                _this.Fecha.FechaFin = moment(data).format("YYYY/MM/DD HH:mm:ss")
                            }*/
                        });
                        $("#FechaFin").data("kendoDateTimePicker").value(kendo.parseDate(moment().format("YYYY/MM/DD HH:mm:ss")));

                        $("#GridFlotas").html("");
                        $("#GridFlotas").kendoGrid({
                            dataSource: {
                                pageSize: 10
                            },
                            toolbar: ["excel", "search"],
                            columns: [
                                {
                                    field: "placa",
                                    title: "Placa",
                                    width: "100px",
                                    attributes: { class: "col-data" },
                                    headerAttributes: { class: "col-data" }
                                },
                                {
                                    field: "nombre",
                                    title: "Nombre",
                                    width: "100px",
                                    attributes: { class: "col-data" },
                                    headerAttributes: { class: "col-data" }
                                },
                                {
                                    field: "nombreSubfleet",
                                    title: "Flota",
                                    width: "100px",
                                    attributes: { class: "col-data" },
                                    headerAttributes: { class: "col-data" }
                                },
                                {
                                    field: "marca",
                                    title: "Marca",
                                    width: "100px",
                                    attributes: { class: "col-data" },
                                    headerAttributes: { class: "col-data" }
                                },
                                {
                                    field: "modelo",
                                    title: "Modelo",
                                    width: "100px",
                                    attributes: { class: "col-data" },
                                    headerAttributes: { class: "col-data" }
                                },
                                {
                                    field: "fechaCreacion",
                                    title: "Fecha de Creacion",
                                    width: "150px",
                                    attributes: { class: "col-data" },
                                    headerAttributes: { class: "col-data" }
                                },
                                {
                                    field: "kmXGalon",
                                    title: "Km. por Gal.",
                                    width: "100px",
                                    attributes: { class: "col-data" },
                                    headerAttributes: { class: "col-data" }
                                },
                                {
                                    field: "capacidadTanque",
                                    title: "Cap. Tanque",
                                    width: "100px",
                                    attributes: { class: "col-data" },
                                    headerAttributes: { class: "col-data" }
                                },
                                {
                                    field: "tipoCombustible",
                                    title: "Tipo Combustible",
                                    width: "100px",
                                    attributes: { class: "col-data" },
                                    headerAttributes: { class: "col-data" }
                                },
                                {
                                    field: "vin",
                                    title: "VIN",
                                    width: "100px",
                                    attributes: { class: "col-data" },
                                    headerAttributes: { class: "col-data" }
                                },
                                {
                                    command: [
                                        {
                                            name: "Editar",
                                            text: '<span> <i class="bi bi-pencil-square"></i> Editar </span>',
                                            click: function (e) {
                                                e.preventDefault();
                                                var tr = $(e.target).closest("tr");
                                                var data = this.dataItem(tr);
                                                console.log(data)
                                                $("#TXT_Auto").text(data.nombre + " - [" + data.placa + "]")

                                                $("#ModalAlert").text("")

                                                $.vehiculo.ActualizarVehiculo.dataActual.mobileID = data.mobileID

                                                $("#Input_Placa").val(data.placa)
                                                $.vehiculo.ActualizarVehiculo.dataActual.placa = data.placa

                                                $("#Input_Nombre").val(data.nombre)
                                                $.vehiculo.ActualizarVehiculo.dataActual.nombre = data.nombre

                                                $("#DropDown_FlotaModal").data("kendoDropDownList").value(data.subfleetID +"")
                                                $.vehiculo.ActualizarVehiculo.dataActual.idFlota = data.subfleetID

                                                $("#Input_Marca").val(data.marca)
                                                $.vehiculo.ActualizarVehiculo.dataActual.marca = data.marca

                                                $("#Input_Modelo").val(data.modelo)
                                                $.vehiculo.ActualizarVehiculo.dataActual.modelo = data.modelo

                                                $("#Input_KmxGal").val(extraerNumeros(data.kmXGalon))
                                                $.vehiculo.ActualizarVehiculo.dataActual.kmXGalon = extraerNumeros(data.kmXGalon)

                                                $("#Input_CapTan").val(extraerNumeros(data.capacidadTanque))
                                                $.vehiculo.ActualizarVehiculo.dataActual.capacidadTanque = extraerNumeros(data.capacidadTanque)

                                                $("#DropDown_GasolinaModal").data("kendoDropDownList").select(data.tipoCombustibleID)
                                                $.vehiculo.ActualizarVehiculo.dataActual.combustibleID = data.tipoCombustibleID

                                                $("#Input_VIN").val(data.vin)
                                                $.vehiculo.ActualizarVehiculo.dataActual.numeroVIN = data.vin

                                                function extraerNumeros(cadena) {
                                                    const regex = /-?\d+(\.\d+)?/g;
                                                    const coincidencias = cadena.match(regex);
                                                    return coincidencias[0] 
                                                }

                                                $('#editarVehiculoModal').modal('show');
                                            }
                                        },
                                        {
                                            name: "Eliminar",
                                            text: '<span> <i class="bi bi-trash3"></i> Eliminar </span>',
                                            click: function (e) {
                                                e.preventDefault();
                                                var tr = $(e.target).closest("tr");
                                                var data = this.dataItem(tr);
                                                //_this.EliminarProyecto(data.idProyectosVales, data.pep)
                                                //console.log(data.mobileID)
                                                $.vehiculo.ActualizarVehiculo.EliminarMobile(data.mobileID)
                                            }
                                        }

                                    ],
                                    attributes: { class: "col-command" },
                                    headerAttributes: { class: "col-command" },
                                }
                            ],
                            mobile: "phone",
                            pageable: true,
                            selectable: false,
                            excel: {
                                fileName: "vehiculos_" + moment().format("YYYYMMDD_HH-mm-ss") + ".xlsx"
                            }

                        });


                        /*var FechaInicio = moment($("#FechaIni").data("kendoDateTimePicker").value()).add(6, 'hours').format("YYYYMMDD HH:mm:ss.SSS")
                        var FechaFin = moment($("#FechaFin").data("kendoDateTimePicker").value()).add(6, 'hours').format("YYYYMMDD HH:mm:ss.SSS")*/
                        /*var FechaInicio = _this.Fecha.FechaInicio
                        var FechaFin = _this.Fecha.FechaFin*/
                        _this.DataGrid(null, null)
                    },
                    DataGrid: function (FechaIni, FechaFin) {

                        var arg = {
                            FechaIni: FechaIni,
                            FechaFin: FechaFin
                        }
                        //console.log(arg)

                        //console.log("XD")
                        $.get('@Url.Action("rpVehiculo", "adVehiculosFlotas")', arg, function (autos) {
                            for (let x = 0; x < autos.length; x++) {
                                
                                autos[x].fechaCreacion = moment(autos[x].fechaCreacion).subtract(6, "hour").format("DD/MM/YYYY HH:mm:ss")
                                autos[x].capacidadTanque = autos[x].capacidadTanque + " Gal"
                                autos[x].kmXGalon = autos[x].kmXGalon + " Km"
                                
                                
                            }
                            console.log(autos)
                            $("#GridFlotas").data("kendoGrid").dataSource.data(autos);
                        });
                    },
                    BTNGeneraGrid: function () {
                        var _this = this;
                        $("#reloadGrid").on("click", function () {
                            var FechaInicio = moment($("#FechaIni").data("kendoDateTimePicker").value()).add(6, 'hours').format("YYYYMMDD HH:mm:ss.SSS")
                            var FechaFin = moment($("#FechaFin").data("kendoDateTimePicker").value()).add(6, 'hours').format("YYYYMMDD HH:mm:ss.SSS")

                            _this.DataGrid(FechaInicio, FechaFin)
                        });

                    },
                },                            
                keypress: function () {
                    $('#Placa, #Input_Placa').keypress(function (event) {

                        var keyCode = event.which;

                        if (!((keyCode >= 48 && keyCode <= 57) ||
                            (keyCode >= 65 && keyCode <= 90) ||
                            (keyCode >= 97 && keyCode <= 122))) {
                            event.preventDefault();
                        }

                    });
                    $('#KmXGalon, #Input_KmxGal').keypress(function (event) {

                        var keyCode = event.which;
                        var inputValue = $(this).val();

                        if ((keyCode < 48 || keyCode > 57) && keyCode !== 46) {
                            event.preventDefault();
                        }
                        
                        if (keyCode === 46 && inputValue.indexOf('.') !== -1) {
                            event.preventDefault();
                        }

                        if (inputValue.indexOf('.') !== -1 && inputValue.split('.')[1].length >= 2 && keyCode !== 46) {
                            event.preventDefault();
                        }

                        var valorActual = parseFloat(inputValue);
                        var tecla = parseInt(event.key)
                        //console.log(valorActual + " " + tecla)
                        if (!isNaN(valorActual) && !isNaN(tecla)) {
                            if (parseFloat(inputValue + event.key) > 5000) {
                                //console.log(valorActual + tecla)
                                event.preventDefault();
                            }
                        }

                    });
                    $('#CapacidadTanque, #Input_CapTan').keypress(function (event) {

                        var keyCode = event.which;
                        var inputValue = $(this).val();

                        if ((keyCode < 48 || keyCode > 57) && keyCode !== 46) {
                            event.preventDefault();
                        }

                        if (keyCode === 46 && inputValue.indexOf('.') !== -1) {
                            event.preventDefault();
                        }

                        if (inputValue.indexOf('.') !== -1 && inputValue.split('.')[1].length >= 2 && keyCode !== 46) {
                            event.preventDefault();
                        }

                        var valorActual = parseFloat(inputValue);
                        var tecla = parseInt(event.key)
                        //console.log(valorActual + " " + tecla)
                        if (!isNaN(valorActual) && !isNaN(tecla)) {
                            if (parseFloat(inputValue + event.key) > 5000) {
                                //console.log(valorActual + tecla)
                                event.preventDefault();
                            }
                        }

                    });
                    $('#Vin, #Input_VIN').keypress(function (event) {

                        var keyCode = event.which;
                        var inputValue = $(this).val();
                        //console.log(inputValue)
                        if (inputValue.length > 16){
                            event.preventDefault();
                        }
                        if (!((keyCode >= 48 && keyCode <= 57) ||
                            (keyCode >= 65 && keyCode <= 90) ||
                            (keyCode >= 97 && keyCode <= 122))) {
                            event.preventDefault();
                        }
                        

                    });
                },
